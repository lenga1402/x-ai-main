{
  "name": "base-t3-web3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "typechain": "typechain --target ethers-v5 --input-dir src/abis --out-dir src/typechain-non-owned './src/abis/*.json'",
    "hardhat-compile": "cd web3 && npx hardhat compile",
    "hardhat-clean": "cd web3 && npx hardhat clean && rm -r temp",
    "deploy-token-bnb": "yarn hardhat-clean && cd web3/scripts && npx --network=bnb hardhat run token.ts --network bnb",
    "deploy-token-bnbt": "yarn hardhat-clean && cd web3/scripts && npx --network=bnbt hardhat run token.ts --network bnbt",
    "deploy-mint-bnb": "yarn hardhat-clean && cd web3/scripts && npx --network=bnb hardhat run mint.ts --network bnb",
    "deploy-mint-bnbt": "yarn hardhat-clean && cd web3/scripts && npx --network=bnbt hardhat run mint.ts --network bnbt",
    "deploy-staking-bnb": "yarn hardhat-clean && cd web3/scripts && npx --network=bnb hardhat run staking.ts --network bnb",
    "deploy-staking-bnbt": "yarn hardhat-clean && cd web3/scripts && npx --network=bnbt hardhat run staking.ts --network bnbt"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
